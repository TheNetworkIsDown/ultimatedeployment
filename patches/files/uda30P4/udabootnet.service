[Unit]
Description=UDA Boot Net Service
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
ExecStart=/var/public/bin/udabootnet.sh
User=root
StandardInput=tty
StandardOutput=tty
StandardError=tty
TTYPath=/dev/tty2
TTYReset=yes
TTYVHangup=yes
#TimeoutStartSec=0

[Install]
WantedBy=default.target
