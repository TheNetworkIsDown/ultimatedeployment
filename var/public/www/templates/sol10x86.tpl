system_locale=en_US
timezone=US/Pacific
terminal=sun-cmd
timeserver=localhost
network_interface=PRIMARY {dhcp protocol_ipv6=no}
name_service=NONE
nfs4_domain=example.com
# The following command hashes the string 'root' 
# echo root | openssl passwd -crypt -stdin -salt 5o
# 5o0y9cIVsyh6o
root_password=5o0y9cIVsyh6o
security_policy=NONE
# You may need to remove the following line for pre-Oracle solaris versions
auto_reg=disable

